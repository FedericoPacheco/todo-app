{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "@sailshq/connect-redis": "^6.1.3",
    "@sailshq/lodash": "^3.10.6",
    "@sailshq/socket.io-redis": "^6.1.2",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^6.1.3",
    "cross-env": "^10.1.0",
    "grunt": "1.0.4",
    "lusca": "^1.7.0",
    "node-pg-migrate": "^8.0.3",
    "redis": "^4.7.0",
    "sails": "^1.5.13",
    "sails-hook-apianalytics": "^2.0.6",
    "sails-hook-grunt": "^5.0.0",
    "sails-hook-orm": "^4.0.3",
    "sails-hook-sockets": "^3.0.0",
    "sails-postgresql": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/mocha-runner": "^8.7.1",
    "chai": "4.5",
    "chai-as-promised": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-config-google": "^0.14.0",
    "faker": "^5.5.3",
    "globals": "^15.14.0",
    "mocha": "^11.0.1",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.1.9",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0"
  },
  "scripts": {
    "db-migrate:create": "node-pg-migrate create",
    "db-migrate:up": "node-pg-migrate up",
    "db-migrate:down": "node-pg-migrate down",
    "start:prod": "cross-env NODE_ENV=production node app.js",
    "start:test": "cross-env NODE_ENV=test npm run db-migrate:up && sails lift",
    "start:dev": "cross-env NODE_ENV=development npm run db-migrate:up && nodemon --legacy-watch --inspect=0.0.0.0:9229 app.js",
    "integration-test": "cross-env NODE_ENV=test mocha --config test/integration/.mocharc --reporterOptions configFile=test/integration/.reportersrc",
    "unit-test": "cross-env NODE_ENV=test mocha --config test/unit/.mocharc --reporterOptions configFile=test/unit/.reportersrc",
    "unit-test:match": "npm run unit-test -- --grep",
    "unit-test:debug": "npm run unit-test -- --inspect-brk",
    "unit-test:mutate": "stryker run test/unit/.strykerrc.json --mutate",
    "unit-test:coverage": "nyc --nycrc-path test/unit/.nycrc npm run unit-test"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/FedericoPacheco/todo-app.git"
  },
  "author": "Federico Ignacio Pacheco Pilan",
  "license": "",
  "engines": {
    "node": "^20.13"
  }
}
