{
  "mutate": ["api/services/**/*.js", "api/controllers/**/*.js"],
  "packageManager": "npm",
  "reporters": ["html", "progress"],
  "coverageAnalysis": "perTest",
  "testRunner": "mocha",
  "mochaOptions": {
    "spec": ["test/unit/**/*.test.js"],
    "ui": "tdd",
    "config": "test/unit/.mocharc"
  },
  "htmlReporter": {
    "fileName": "test/reports/unit-mutation/results.html"
  },
  "incremental": true,
  "incrementalFile": "test/reports/.tmp/stryker/incremental.json"
}
