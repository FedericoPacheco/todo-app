{
  "mutate": [
    "api/services/**/*.js",
    "api/controllers/**/*.js"
  ],
  "packageManager": "npm",
  "reporters": ["html", "progress"],
  "coverageAnalysis": "perTest",
  "testRunner": "mocha",
  "mochaOptions": {
    "spec": ["test/unit/**/*.test.js"],
    "ui": "tdd",
    "config": "test/unit/.mocharc.js"
  },
  "htmlReporter": {
    "fileName": "test/reports/mutation.html"
  },
  "incremental": true,
  "incrementalFile": "test/reports/.tmp/stryker/incremental.json"
}
